{
  "name": "sentenced",
  "version": "0.0.1",
  "description": "Data driven time-line app on incarceration rates that includes policy changes",
  "main": "index.js",
  "scripts": {
    "solid": "solid start --port 8443 --ssl-cert ./etc/localhost.cert --ssl-key ./etc/localhost.key --root ./dist -v",
    "start": "gulp",
    "start:wallet": "gulp wallet",
    "build:prod": "npm i --production && gulp build",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:phantomjs": "npm run test -- --browsers PhantomJS",
    "test:chrome": "npm run test -- --browsers Chrome",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "lint": "eslint src/js/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rawild/sentenced"
  },
  "keywords": [
    "incarceration",
    "visualizaton",
    "policy"
  ],
  "author": "Annalisa Wilde",
  "bugs": {
    "url": "https://github.com/rawild/sentenced/issues"
  },
  "homepage": "https://github.com/rawild/sentenced#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-survivejs-kanban": "^0.3.3",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-jsx": "^2.0.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sync": "^0.1.4",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "react-addons-test-utils": "15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "redux-devtools": "^3.3.2",
    "redux-devtools-extension": "^2.0.0",
    "redux-mock-store": "^1.2.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^3.31.0"
  },
  "dependencies": {
    "attr-accept": "^1.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "jquery": "^2.1.4",
    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.4.0",
